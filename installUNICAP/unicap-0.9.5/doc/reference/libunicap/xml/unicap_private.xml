<refsect2 id="unicap-handle" role="struct">
<title>struct unicap_handle</title>
<indexterm zone="unicap-handle"><primary>unicap_handle</primary></indexterm><programlisting>struct unicap_handle {
      unicap_device_t device;      
		
      struct _unicap_cpi cpi;

      void *dlhandle;
      void *cpi_data;
      
      unsigned int cpi_flags;
      
      key_t sem_key;
      int   sem_id;
      
      int stream_sem_id;
      int property_sem_id;
		
      int * ref_count;
		
/* 		struct _unicap_shm_reg *shm_mmap;		 */
/* 		int shm_fd; */
/* 		key_t shm_sem_key; */
/* 		int shm_sem_id; */

      int have_stream_lock;
      int temporary_stream_lock;
      int have_ppty_lock;
      int stream_lock_fd;
      int ppty_lock_fd;
      char device_uid[512];

      struct _unicap_callback_info *cb_info;
};
</programlisting>
<para>

</para></refsect2>
